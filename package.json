{
  "name": "protoc-gen-ts",
  "description": "Compile proto files to Typescript.",
  "version": "",
  "license": "MIT",
  "author": {
    "email": "thesayyn@gmail.com",
    "name": "thesayyn",
    "url": "https://github.com/thesayyn"
  },
  "scripts": {
    "bazel": "bazelisk",
    "test": "bazel build //test:codegen && node ./scripts/sync_generated_protos.js test && bazel test //test/... --test_output=errors",
    "release": "bazel run //:npm_package.publish --access public --tag latest",
    "codegen": "node ./scripts/sync_compiler_protos.js && bazel build //src/compiler:codegen && node ./scripts/sync_generated_protos.js src/compiler && bazel build //src/compiler && node ./scripts/sync_generated_protos.js src/compiler dts"
  },
  "bin": {
    "protoc-gen-ts": "./bin/protoc-gen-ts"
  },
  "peerDependencies": {
    "typescript": "^4.0.2",
    "google-protobuf": "^3.13.0"
  },
  "devDependencies": {
    "@bazel/bazelisk": "^1.7.5",
    "@bazel/jasmine": "^3.2.2",
    "@bazel/typescript": "^3.2.2",
    "@types/google-protobuf": "^3.2.7",
    "@types/jasmine": "^3.5.9",
    "@types/node": "^12.12.2",
    "google-protobuf": "^3.11.4",
    "@grpc/grpc-js": "^1.2.10",
    "jasmine": "^3.7.0",
    "jasmine-core": "^3.7.1",
    "typescript": "^4.0.2"
  }
}
